{
  "setup_config":{
    "seed": 199989,
    "diff_seed": false,
    "per_device_train_batch_size": 64,
    "per_device_valid_batch_size": 64,
    "gradient_accumulation_steps": 1,
    "num_workers": 12,
    "method": "Classification",
    "max_epoch": 200,
    "lossfun": "CEL",
    "load_from": false,
    "if_continue": false,
    "regularization": 0.0,
    "if_display_method_info": true,
    "mem_log": false,
    "empty_cache": true,
    "metrics":["ACC", "PREC", "RECALL", "F1"],
    "drop_last": false
  },
  "data_config": {
    "data_path": "../dataset.npy",
    "num_classes": 64,
    "samples_per_class": 600,
    "total_channels": 10,
    "select_channel": [0,1,2,3,4,5],
    "sequence_length": 3000,
    "train_rate": 0.6,
    "val_rate": 0.15
  },
  "optim_config": {
    "optim": "Adamw",
    "lr": 1e-5,
    "filter_bias_and_bn": false,
    "log_step": 1,
    "opt_eps": null,
    "opt_betas": null,
    "momentum": 0.9,
    "weight_decay": 0.01,
    "early_stop_epoch": -1
  },
  "sched_config": {
    "sched": "onecycle",
    "decay_rate": 0.1,
    "decay_epoch": 300,
    "lr_k_decay": 1.0,
    "final_div_factor": 1e4
  },
  "model_config": {
    "model": "easyGRU",
    "hidden_size": 512,
    "num_layers": 1,
    "num_conv_layer": 3,
    "conv_outchannel":512,
    "conv_strid_size":128
  },
  "ds_config":{
    "offload": false,
    "zero_stage": 0,
    "gradient_accumulation_steps": 1,
    "gradient_clipping": 2.0
}
}
